{
  "name": "fusou-app",
  "private": true,
  "version": "0.3.1",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "preview": "serve dist",
    "format": "prettier --check ./src/**/*.{ts,tsx}",
    "format:fix": "prettier --write ./src/**/*.{ts,tsx}",
    "lint": "eslint ./src/**/*.{ts,tsx}",
    "lint:fix": "eslint --fix ./src/**/*.{ts,tsx}",
    "clippy": "cd src-tauri && cargo clippy -- -A clippy::needless_return -A clippy::ptr_arg -A clippy::expect_fun_call",
    "clippy:fix": "cd src-tauri && cargo clippy --fix -- -A clippy::needless_return -A clippy::ptr_arg -A clippy::expect_fun_call",
    "rustfmt": "cd src-tauri && cargo fmt --all --check --verbose",
    "rustfmt:fix": "cd src-tauri && cargo fmt --all",
    "tauri": "dotenvx run -fk ../.env.keys -f .env -f src-tauri/.env --verbose --overload -- tauri",
    "check.tsc": "tsc --noEmit",
    "storybook.build": "storybook build",
    "storybook": "storybook dev -p 6007",
    "update-shared-ui": "pnpm up shared-ui"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.0",
    "@eslint/js": "^9.23.0",
    "@storybook/addon-a11y": "^9.1.0",
    "@storybook/addon-docs": "^9.1.0",
    "@storybook/addon-links": "^9.1.0",
    "@storybook/addon-onboarding": "^9.1.0",
    "@storybook/addon-styling-webpack": "^2.0.0",
    "@storybook/addon-themes": "^9.1.0",
    "@storybook/addon-vitest": "^9.1.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@tauri-apps/cli": "^2.4.0",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "daisyui": "^5.0.46",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-solid": "^0.14.5",
    "globals": "^16.3.0",
    "postcss": "^8.5.6",
    "prettier": "^3.5.3",
    "storybook": "^9.1.0",
    "storybook-solidjs-vite": "^9.0.2",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.28.0",
    "vite": "^6.2.3",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-solid": "^2.11.6",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@solid-primitives/cookies": "^0.0.1",
    "@solidjs/router": "^0.15.3",
    "@supabase/supabase-js": "^2.49.4",
    "@tauri-apps/api": "^2.4.0",
    "@tauri-apps/plugin-deep-link": "^2.2.1",
    "@tauri-apps/plugin-process": "^2.3.0",
    "@tauri-apps/plugin-shell": "^2.3.1",
    "@tauri-apps/plugin-updater": "^2.9.0",
    "dotenv": "^16.5.0",
    "shared-ui": "workspace:*",
    "solid-js": "^1.9.5",
    "virtua": "^0.41.5"
  }
}
