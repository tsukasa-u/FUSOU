{
  "identifier": "migrated",
  "description": "permissions that were migrated from v1",
  "local": true,
  "windows": ["main"],
  "permissions": [
    "core:default",
    "deep-link:allow-get-current",
    "deep-link:default",
    "deep-link:allow-register",
    "log:default",
    "window-state:default",
    "shell:default",
    {
      "identifier": "fs:scope",
      "allow": [
        {
          "path": "$APPDATA"
        },
        {
          "path": "$APPDATA/**"
        },
        {
          "path": "$RESOURCE"
        },
        {
          "path": "$RESOURCE/**"
        },
        {
          "path": "$HOME"
        },
        {
          "path": "$HOME/**"
        }
      ]
    },
    "fs:default",
    {
      "identifier": "opener:allow-open-path",
      "allow": [
        {
          "path": "./../user/configs.toml"
        },
        {
          "path": "$RESOURCE/resources/user/configs.toml"
        }
      ]
    },
    "dialog:default",
    "updater:default",
    "process:default",
    "process:allow-restart",
    "autostart:default",
    "notification:default"
  ]
}
